{
	"name": "@ironm00n/nbt-ts",
	"version": "1.4.0",
	"description": "An easy to use encoder and decoder for the NBT format",
	"main": "lib/cjs/index.js",
  "module": "lib/esm/index.js",
  "types": "lib/esm/index.d.ts",
	"repository": {
		"type": "git",
		"url": "https://github.com/ironmoon/nbt-ts.git"
